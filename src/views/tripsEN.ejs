        <script>
      document.addEventListener("DOMContentLoaded", function () {
                let selectedGoTrip = null; // Stores the selected Go trip
                let selectedBackTrip = null; // Stores the selected Back trip


 
                document.querySelectorAll(".trip-route-component:first-of-type .btn-container button").forEach(button => {
                    button.addEventListener("click", function (event) {
                        event.preventDefault(); // Prevent form submission
                        
                        let form = this.closest("form");
                        let tripId = form.querySelector('input[name="trip_id1"]').value;
                        let buttonId = `gotrip-${tripId}`;
        
                        if (!selectedGoTrip) {
                            selectedGoTrip = {}; // Store Go trip details
        
                            let formData = new FormData(form);
                            formData.forEach((value, key) => {
                                selectedGoTrip[key] = value;
                            });

                            
        
                            console.log("Selected Go Trip:", selectedGoTrip); // Debugging purposes

                            
                            if(selectedGoTrip) {
                                document.querySelectorAll("[id^='gotrip-']").forEach(button => {
                          button.disabled = true;
                          button.classList.add("disabled-button"); 
                                    });
        
                            }
        
                            // Disable other buttons with the same GotripID
                            document.querySelectorAll(`#${buttonId}`).forEach(btn => {
                                if (btn !== this) {
                                    btn.disabled = true;
                                    btn.classList.add("disabled-button");
                                }
                            });
        
                            this.textContent = "Selected";
                            this.classList.add("selected-button");

                            
                 


                           
                            

                            // Check if both trips are selected, then submit
                            if (selectedGoTrip && selectedBackTrip) {
                                submitBothTrips();
                            }
                        }
                    });
                });
        
                document.querySelectorAll(".trip-route-component:last-of-type .btn-container button").forEach(button => {
                    button.addEventListener("click", function (event) {
                        event.preventDefault(); // Prevent form submission
                        
                        let form = this.closest("form");
                        let tripId = form.querySelector('input[name="trip_id2"]').value;
                        let buttonId = `backtrip-${tripId}`;
        
                        if (!selectedBackTrip) {
                            selectedBackTrip = {}; // Store Back trip details
        
                            let formData = new FormData(form);
                            formData.forEach((value, key) => {
                                selectedBackTrip[key] = value;
                            });

                            if(selectedBackTrip) {

                                document.querySelectorAll("[id^='backtrip-']").forEach(button => {
                          button.disabled = true;
                          button.classList.add("disabled-button");
                                    });
        
                            }
                            
        
                            console.log("Selected Back Trip:", selectedBackTrip); // Debugging purposes
        
                            // Disable other buttons with the same BacktripID
                            document.querySelectorAll(`#${buttonId}`).forEach(btn => {
                                if (btn !== this) {
                                    btn.disabled = true;
                                    btn.classList.add("disabled-button");
                                }
                            });
        
                            this.textContent = "تم الاختيار";
                            this.classList.add("selected-button");
        
                            // Check if both trips are selected, then submit
                            if (selectedGoTrip && selectedBackTrip) {
                                submitBothTrips();
                            }
                        }
                    });
                });
        
                function submitBothTrips() {
                    let form = document.createElement("form");
                    form.method = "POST";
                    form.action = "/en/selectdoubletrip";
        
                    // Append Go trip data
                    for (let key in selectedGoTrip) {
                        let input = document.createElement("input");
                        input.type = "hidden";
                        input.name = key;
                        input.value = selectedGoTrip[key];
                        form.appendChild(input);
                    }
        
                    // Append Back trip data
                    for (let key in selectedBackTrip) {
                        let input = document.createElement("input");
                        input.type = "hidden";
                        input.name = key;
                        input.value = selectedBackTrip[key];
                        form.appendChild(input);
                    }
        
                    document.body.appendChild(form);
                    form.submit(); // Submit form when both trips are selected
                }
            });
        </script>
        
        <style>
            .disabled-button {
                background-color: gray !important;
                cursor: not-allowed !important;
            }
        
            .selected-button {
                background-color: #28a745 !important; /* Keep original styling */
                color: white !important;
                font-weight: bold;
            }
        </style>
        

