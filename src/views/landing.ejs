<style>
  .language-switch {
    display: flex;
    flex-direction: column;
    align-items: center;
    justify-content: center;
    text-align: center;
    margin-right: 10px;
}

.lang-btn {
    color: white;
    padding: 8px 15px;
    border-radius: 5px;
    text-decoration: none;
    font-weight: bold;
    transition: 0.3s ease;
    display: flex;
    flex-direction: column;
    align-items: center;
}

.lang-btn i {
    font-size: 20px; /* Adjust icon size */
    margin-bottom: 5px; /* Space between icon and text */
}

.lang-btn:hover {
    background-color: #0b2d4a;
}
.ui-datepicker .ui-datepicker-prev,
.ui-datepicker .ui-datepicker-next {
    display: block !important;
    width: 25px !important;
    height: 25px !important;
    background-color: #ccc !important; /* Add background to make them stand out */
    border-radius: 50%;
    position: absolute !important;
    top: 50%;
    transform: translateY(-50%);
}

.ui-datepicker .ui-datepicker-prev {
    left: 5px !important;
}

.ui-datepicker .ui-datepicker-next {
    right: 5px !important;
}

/* Ensure arrows have proper content */
.ui-datepicker .ui-datepicker-prev:before {
    content: "◀"; /* Unicode Left Arrow */
    font-size: 18px;
    color: black;
    position: absolute;
    left: 50%;
    top: 50%;
    transform: translate(-50%, -50%);
}

.ui-datepicker .ui-datepicker-next:before {
    content: "▶"; /* Unicode Right Arrow */
    font-size: 18px;
    color: black;
    position: absolute;
    left: 50%;
    top: 50%;
    transform: translate(-50%, -50%);
}
 




</style>

