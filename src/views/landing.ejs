    <script>
      function setupDropdown(dropdownId,selectedItemsId,otherDropdownId,otherSelectedId){const dropdown=document.getElementById(dropdownId);const selectedItems=document.getElementById(selectedItemsId);const otherDropdown=document.getElementById(otherDropdownId);const otherSelectedItems=document.getElementById(otherSelectedId);let selectedItemValue=null;selectedItems.addEventListener('click',()=>{if(selectedItemValue){removeItem(selectedItems,dropdown,otherDropdown,otherSelectedItems,selectedItemValue);selectedItemValue=null}else{dropdown.style.display=dropdown.style.display==='block'?'none':'block'}});document.addEventListener('click',(event)=>{if(!selectedItems.contains(event.target)&&!dropdown.contains(event.target)){dropdown.style.display='none'}});dropdown.querySelectorAll('.itemSelect').forEach(item=>{item.addEventListener('click',function(e){if(e.target===item){if(item.classList.contains('has-submenu')){showSubmenu(item)}else{selectItem(selectedItems,dropdown,otherDropdown,otherSelectedItems,this)}}})});dropdown.querySelectorAll('.back-button').forEach(button=>{button.addEventListener('click',function(){goBackToMainMenu(dropdown)})});function showSubmenu(item){const submenu=item.querySelector('.submenu');submenu.style.display=submenu.style.display==='block'?'none':'block';submenu.addEventListener('click',function(e){if(e.target===submenu){e.stopPropagation()}})}
function selectItem(selectedItems,dropdown,otherDropdown,otherSelectedItems,item){let value=item.dataset.value;if(selectedItemValue!==null){let otherItem=otherDropdown.querySelector(`[data-value="${selectedItemValue}"]`);if(otherItem){otherItem.style.display='block'}}
selectedItemValue=value;selectedItems.innerHTML="";let span=document.createElement('span');span.textContent=item.textContent;span.dataset.value=value;span.addEventListener('click',function(){removeItem(selectedItems,dropdown,otherDropdown,otherSelectedItems,value)});selectedItems.appendChild(span);markSelected(item,dropdown);dropdown.style.display='none';let parentItem=item.closest('.has-submenu');if(parentItem){let parentValue=parentItem.dataset.value;let otherParentItem=otherDropdown.querySelector(`[data-value="${parentValue}"]`);if(otherParentItem){otherParentItem.style.display='none'}}
removeFromOtherDropdown(value,otherDropdown,otherSelectedItems)}
function markSelected(item,dropdown){dropdown.querySelectorAll('.itemSelect').forEach(div=>div.classList.remove('selected'));item.classList.add('selected')}
function removeFromOtherDropdown(value,otherDropdown,otherSelectedItems){let otherItem=otherDropdown.querySelector(`[data-value="${value}"]`);if(otherItem){otherItem.style.display='none'}
let otherSelectedSpan=otherSelectedItems.querySelector(`span[data-value="${value}"]`);if(otherSelectedSpan){otherSelectedSpan.remove()}}
function removeItem(selectedItems,dropdown,otherDropdown,otherSelectedItems,value){let otherItem=otherDropdown.querySelector(`[data-value="${value}"]`);if(otherItem){otherItem.style.display='block'}
let itemInDropdown=dropdown.querySelector(`[data-value="${value}"]`);if(itemInDropdown){let parentItem=itemInDropdown.closest('.has-submenu');if(parentItem){let parentValue=parentItem.dataset.value;let otherParentItem=otherDropdown.querySelector(`[data-value="${parentValue}"]`);if(otherParentItem){otherParentItem.style.display='block'}
const submenu=parentItem.querySelector('.submenu');if(submenu){submenu.style.display='block'}}}
selectedItems.innerHTML="اضغط للاختيار...";dropdown.querySelectorAll('.itemSelect').forEach(div=>div.classList.remove('selected'))}
function goBackToMainMenu(dropdown){const submenus=dropdown.querySelectorAll('.submenu');submenus.forEach(submenu=>submenu.style.display='none');dropdown.style.display='block'}}
setupDropdown("dropdownList1","selectedItems1","dropdownList2","selectedItems2");setupDropdown("dropdownList2","selectedItems2","dropdownList1","selectedItems1");document.addEventListener('DOMContentLoaded',()=>{const minusButton=document.querySelector('.qty-btn-minus');const plusButton=document.querySelector('.qty-btn-plus');const inputQty=document.querySelector('.input-qty');if(minusButton&&plusButton&&inputQty){minusButton.addEventListener('click',()=>{let currentValue=parseInt(inputQty.value);if(currentValue>1){inputQty.value=currentValue-1}});plusButton.addEventListener('click',()=>{let currentValue=parseInt(inputQty.value);inputQty.value=currentValue+1})}else{console.error('عناصر زيادة أو نقصان العدد غير موجودة!')}})
        
            </script>
        
</body>

</html>