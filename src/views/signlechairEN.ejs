    <script>

document.addEventListener('DOMContentLoaded', () => {
    // Get elements
    const chairs = document.querySelectorAll('.chair-h input[type="checkbox"]');
    const selectedQty = parseInt(document.getElementById('chair-container').dataset.maxChairs || 1);
    const ChairPrice = parseInt(document.getElementById('price-container').dataset.maxPrice || 1);
    const vouchers = JSON.parse(document.getElementById('voucher-container').dataset.maxVoucher); // Parse JSON
    const confirmButton = document.getElementById("btn-one");
    const discountInput = document.getElementById("discountInput"); 
    const discountButton = document.getElementById("discountButton"); 
    const oldPriceElement = document.getElementById("oldPrice");
    const totalPriceElement = document.getElementById("totalPriceee");
    const termsCheckbox = document.getElementById("termsCheckbox");
    const bookButton = document.getElementById("btn-two");
    const payButton = document.getElementById("payButton");
   // const paymentMethods = document.querySelectorAll('input[name="action"]');
    const sections = document.querySelectorAll('.show-hide');
    const paymentOptions = document.querySelectorAll('input[name="action"]');

    //hidden inputs
    const visibleNameInput = document.getElementById("visibleNameInput");
    const hiddenNameInput = document.getElementById("nameInput");
    const visiblePhoneInput = document.getElementById("visiblePhoneInput") ;
    const hiddenphoneInput = document.getElementById("phoneInput") ;
    const hiddenvoucherInput = document.getElementById("voucherInput") ;
    const hiddenchairsInput = document.getElementById("chairsInput") ;
    const visibleNameInput2 = document.getElementById("visibleNameInput2");
    const visiblePhoneInput2 = document.getElementById("visiblePhoneInput2") ;
    const visibleNameInput3 = document.getElementById("visibleNameInput3");
    const visiblePhoneInput3 = document.getElementById("visiblePhoneInput3") ;

    let selectedChairs = [];
    let discountPercentage = 0;
    let discountMaxValue = 0;

          //disable all chairs
          function uncheckChairs() {
            for(let i = 1 ; i <= 49 ; i++) {
                document.querySelector(`input[name="radio${i}"]`).checked = false;
            }
        }

         //disable all chairs
         function disableChairs() {
            for(let i = 1 ; i <= 49 ; i++) {
                document.querySelector(`input[name="radio${i}"]`).disabled = true;
            }
        }
        
        window.addEventListener("pageshow", function(event) {
            uncheckChairs();
            confirmButton.disabled = true;
        bookButton.disabled = true;
        payButton.disabled = true;
        steptwo.style.display = "none" ;
        stepthree.style.display = "none" ;
});
      



    //payment method function
    function getSelectedPaymentMethod() {
        const selectedMethod = document.querySelector('input[name="action"]:checked');
        return selectedMethod ? selectedMethod.value : null; 
    }

 

     // Validation function
     function validateInputs() {
        const nameValid = hiddenNameInput.value.trim().length >= 7;
        const phoneValid = hiddenphoneInput.value.trim().length == 11; // Ensures exactly 11 digits
        
        // Enable pay button only if both validations pass
        payButton.disabled = !(nameValid && phoneValid);
    }



//hidden inputs 

 //name
visibleNameInput.addEventListener("input", () => {
    let method = getSelectedPaymentMethod() ;
    if(method == 'visa') {
        hiddenNameInput.value = visibleNameInput.value;
        validateInputs() ;
    }
    
     
 });

 visibleNameInput2.addEventListener("input", () => {
    let method = getSelectedPaymentMethod() ;
    if(method == 'VFcash') {
      hiddenNameInput.value = visibleNameInput2.value;
      validateInputs() ;
    }
     
 });

 visibleNameInput3.addEventListener("input", () => {
    let method = getSelectedPaymentMethod() ;
    if(method == 'cash') {
      hiddenNameInput.value = visibleNameInput3.value;
      validateInputs() ;
    }
     
 });

  //phone
  visiblePhoneInput.addEventListener("input", () => {
    let method = getSelectedPaymentMethod() ;
    if(method == 'visa') {
      hiddenphoneInput.value = visiblePhoneInput.value;
      validateInputs() ;
    }
 });

 visiblePhoneInput2.addEventListener("input", () => {
    let method = getSelectedPaymentMethod() ;
    if(method == 'VFcash') {
      hiddenphoneInput.value = visiblePhoneInput2.value;
      validateInputs() ;
    }
 });

 visiblePhoneInput3.addEventListener("input", () => {
    let method = getSelectedPaymentMethod() ;
    if(method == 'cash') {
      hiddenphoneInput.value = visiblePhoneInput3.value;
      validateInputs() ;
    }
 });

 // On page load, set hidden inputs based on selected payment method
 window.addEventListener('load', () => {
    let method = getSelectedPaymentMethod();
    if (method == 'visa') {
        hiddenNameInput.value = visibleNameInput.value;
        hiddenphoneInput.value = visiblePhoneInput.value;
    } else if (method == 'VFcash') {
        hiddenNameInput.value = visibleNameInput2.value;
        hiddenphoneInput.value = visiblePhoneInput2.value;
    } else if (method == 'cash') {
        hiddenNameInput.value = visibleNameInput3.value;
        hiddenphoneInput.value = visiblePhoneInput3.value;
    }
    validateInputs();
 });


    // Function to update button state
    function updatePolicyState() {
        bookButton.disabled = !termsCheckbox.checked;
    }

    // Function to update total price
    function updateTotalPrice() {
        let totalPrice = ChairPrice * selectedChairs.length;
        
        if (discountPercentage > 0) {
            let discountValue = totalPrice * (discountPercentage / 100);
            
            // Apply maximum discount limit
            if (discountValue > discountMaxValue) {
                discountValue = discountMaxValue;
            }
            
            totalPrice -= discountValue; // Apply discount
        }

        totalPriceElement.textContent = `EGP ${totalPrice.toFixed(2)}`; // Update displayed price
    }

    termsCheckbox.addEventListener("change", updatePolicyState);

    // Apply discount when button is clicked
    discountButton.addEventListener("click", () => {
        const discountCode = discountInput.value.trim();
        
        // Find the voucher in the array
        const voucher = vouchers.find(v => v.code === discountCode);
        
        if (voucher) {
            discountPercentage = voucher.percentage;
            discountMaxValue = voucher.maximum;

            if (selectedChairs.length !== selectedQty) {
                alert(`Please select ${selectedQty} chairs`);
            } else {
                discountButton.style.display = "none";
                discountInput.readOnly = true;
                hiddenvoucherInput.value = voucher.code ;
                alert(`A discount of ${discountPercentage}% has been applied with a maximum value of ${discountMaxValue} EGP!`);
                
                // Show old price before applying discount
                oldPriceElement.textContent = `EGP ${ChairPrice * selectedChairs.length}`;
                oldPriceElement.style.display = "block";

                updateTotalPrice();
                disableChairs();
            }
        } else {
            discountPercentage = 0; // Reset discount if code is invalid
            discountMaxValue = 0;
            alert("Invalid discount code or it has been used up, please try again.");
        }
    });




    // Allow selecting only the allowed number of chairs and calculate price
    chairs.forEach((chair) => {
        chair.addEventListener('change', () => {
            if (chair.checked) {
                selectedChairs.push(chair);
            } else {
                selectedChairs = selectedChairs.filter(item => item !== chair);
            }

            // If the limit is exceeded, unselect the oldest one
            if (selectedChairs.length > selectedQty) {
                const removedChair = selectedChairs.shift();
                removedChair.checked = false;
            }

            hiddenchairsInput.value = JSON.stringify(
    selectedChairs.map(chair => parseInt(chair.name.replace(/\D/g, ''), 10))
);

            totalPriceElement.textContent = `EGP ${ChairPrice * selectedChairs.length}`;
            confirmButton.disabled = selectedChairs.length !== selectedQty;

        });
    });

    confirmButton.disabled = selectedChairs.length !== selectedQty;

    
        
confirmButton.addEventListener("click" , () => {
    steptwo.style.display = "block" ;
})


bookButton.addEventListener("click" , () => {
    stepthree.style.display = "block" ;
   
})


termsCheckbox.addEventListener("change", updatePolicyState);


paymentOptions.forEach(option => {
            option.addEventListener('change', function () {
                sections.forEach(section => section.style.display = 'none'); // Hide all sections

                const selectedMethod = this.value; // Get selected payment method
                const selectedSection = document.getElementById(selectedMethod); // Find corresponding section

                if (selectedSection) {
                    selectedSection.style.display = 'block'; // Show the corresponding section
                }
            });
        });

confirmButton.disabled = true;
bookButton.disabled = true;
payButton.disabled = true;
});

    </script>
    
</body>

