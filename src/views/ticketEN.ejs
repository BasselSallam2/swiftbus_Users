    <main class="main-content">
        <div style="text-align: center; margin: 20px 0;">
            <a href="#" id="english-ticket-btn" class="btn-english-ticket">
                اضغط هنا للحصول علي التذكرة باللغة العربية
            </a>
        </div>

        <style>
            .btn-english-ticket {
                display: inline-block;
                background-color: #0D3558; /* Dark blue */
                color: white;
                font-size: 18px;
                padding: 10px 20px;
                border-radius: 5px;
                text-decoration: none;
                font-weight: bold;
                transition: 0.3s;
            }
            
            .btn-english-ticket:hover {
                background-color: #092649; /* Darker blue */
            }
        </style>
        
        <script>
            document.addEventListener("DOMContentLoaded", function() {
                var btn = document.getElementById("english-ticket-btn");
                var currentUrl = window.location.href;

                // Extracting the base URL and query parameters
                var url = new URL(currentUrl);
                var pathname = url.pathname; // e.g., "/en/ticket"
                var searchParams = url.search; // e.g., "?id=1740160701421687"

                // Ensure "/en/" is removed correctly before "ticket"
                if (pathname.startsWith("/en/")) {
                    var newPath = pathname.replace("/en", "");
                    btn.href = newPath + searchParams;
                } else {
                    btn.href = currentUrl;
                }
            });
        </script>

